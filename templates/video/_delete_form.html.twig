<form method="post" action="{{ path('app_video_delete', {'id': video.id}) }}" onsubmit="return confirm('Vous étés sur que vous voulez supprimer cette viéo?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ video.id) }}">
    <button class="btn btn-primary">Supprimer</button>
</form>